{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "rootDir": "ci",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "outDir": "./dist/",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
